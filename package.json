{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "React SSR Boilerplate",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:buildServer": "webpack --config=config/webpack.server.js --watch --progress --colors",
    "dev:buildClient": "webpack --config=config/webpack.client.js --watch --progress --colors",
    "dev:server": "nodemon --watch build --exec \"node build/server-bundle.js\""
  },
  "keywords": [],
