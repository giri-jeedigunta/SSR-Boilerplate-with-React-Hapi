  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@hapi/inert": "^5.2.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "chalk": "^2.4.2",
    "core-js": "3",
    "eslint": "^6.5.1",
    "nodemon": "^1.19.3",
    "npm-run-all": "^4.1.5",
    "progress-bar-webpack-plugin": "^1.12.1",
    "react-router-dom": "^5.1.2",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.3.9",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "@hapi/hapi": "^18.4.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  }
}
